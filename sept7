#include<iostream>
#include<iomanip>

using namespace std;

int main(){

    int x= 1 ,num,i,j;
    
    cout <<"Enter Number : ";
    cin >> num ;
    
    cout <<endl;

    for(i = 0; i < num; i++){
        for(j = 0; j<= i; j++){
            cout << x << " ";
            x++;
        }
        cout << endl;
    }
}
